{
  "name": "Orbit",
  "version": "0.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bundle": "WEBPACK_ENV=dist webpack -p | ./reporter-filter",
    "bundle:dev": "webpack -d | ./reporter-filter",
    "dev:tasks": "npm run bundle:dev --loglevel=silent & npm run test --loglevel=silent & npm run coverage --loglevel=silent",
    "dev": "/install/node_modules/.bin/watch 'npm run dev:tasks' ./orbit/src",
    "watch": "webpack -d --watch | ./reporter-filter",
    "jshint": "/install/node_modules/jshint/bin/jshint ./orbit/src",
    "jscs": "/install/node_modules/jscs/bin/jscs ./orbit/src",
    "jscsfix": "/install/node_modules/jscs/bin/jscs ./orbit/src --fix",
    "unit": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/unit/*.js",
    "integration": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/integration/*.js",
    "stress": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/stress/*.js",
		"concurrency": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/concurrency/*.js",
    "unit-pretty": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/unit/*.js | /install/node_modules/.bin/faucet",
    "integration-pretty": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/integration/*.js | /install/node_modules/.bin/faucet",
    "stress-pretty": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/stress/*.js | /install/node_modules/.bin/faucet",
		"concurrency-pretty": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/concurrency/*.js | /install/node_modules/.bin/faucet",
    "unit-nyan": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/unit/*.js | /install/node_modules/.bin/tap-nyan",
    "integration-nyan": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/integration/*.js | /install/node_modules/.bin/tap-nyan",
    "stress-nyan": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/stress/*.js | /install/node_modules/.bin/tap-nyan",
		"concurrency-nyan": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/blue-tape ./orbit/tests/concurrency/*.js | /install/node_modules/.bin/tap-nyan",
    "unit-coverage": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/babel-istanbul cover /install/node_modules/.bin/blue-tape ./orbit/tests/unit/*.js",
    "integration-coverage": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/babel-istanbul cover /install/node_modules/.bin/blue-tape ./orbit/tests/integration/*.js",
    "coverage": "/install/node_modules/.bin/babel-node /install/node_modules/.bin/babel-istanbul cover /install/node_modules/.bin/blue-tape ./orbit/tests/**/*.js",
    "test": "npm run unit-pretty && npm run integration-pretty"
  },
  "author": "Caio Vaccaro, Jonas Mendes",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.6.4",
    "babel-core": "6.0.20",
    "babel-eslint": "5.0.0-beta6",
    "babel-istanbul": "0.6.1",
    "babel-loader": "6.0.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.0.15",
    "babel-preset-stage-2": "6.3.13",
    "blue-tape": "0.1.11",
    "browser-sync": "2.10.1",
    "browser-sync-webpack-plugin": "1.0.1",
    "extract-text-webpack-plugin": "0.9.1",
    "faucet": "0.0.1",
    "jscs": "2.10.1",
    "jshint": "2.9.1",
    "lodash": "4.6.1",
    "node-libs-browser": "0.5.3",
    "once": "1.3.3",
    "split": "1.0.0",
    "style-loader": "0.13.0",
    "tap-difflet": "0.4.0",
    "tap-nyan": "0.0.2",
    "through2": "2.0.0",
    "watch": "0.17.1",
    "webpack": "1.12.9"
  }
}
