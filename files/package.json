{
  "name": "Orbit",
  "version": "0.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bundle": "webpack -d --watch | ./reporter-filter",
    "jshint": "./node_modules/jshint/bin/jshint ./orbit/src",
    "jscs": "./node_modules/jscs/bin/jscs ./orbit/src",
    "jscsfix": "./node_modules/jscs/bin/jscs ./orbit/src --fix",
    "unit": "./node_modules/.bin/babel-tape-runner orbit/tests/unit/*.js",
    "integration": "./node_modules/.bin/blue-tape orbit/tests/integration/*.js",
    "unit-pretty": "./node_modules/.bin/babel-tape-runner orbit/tests/unit/*.js | ./node_modules/.bin/faucet",
    "integration-pretty": "./node_modules/.bin/blue-tape orbit/tests/integration/*.js | ./node_modules/.bin/faucet",
    "unit-diff": "./node_modules/.bin/babel-tape-runner orbit/tests/unit/*.js | ./node_modules/.bin/tap-difflet",
    "integration-diff": "./node_modules/.bin/blue-tape orbit/tests/integration/*.js | ./node_modules/.bin/tap-difflet",
    "unit-nyan": "./node_modules/.bin/babel-tape-runner orbit/tests/unit/*.js | ./node_modules/.bin/tap-nyan",
    "integration-nyan": "./node_modules/.bin/blue-tape orbit/tests/integration/*.js | ./node_modules/.bin/tap-nyan",
    "unit-coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover orbit/tests/unit/*.js"
  },
  "author": "Caio Vaccaro, Jonas Mendes",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "6.0.20",
    "babel-eslint": "5.0.0-beta6",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "6.0.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.0.15",
    "babel-preset-stage-2": "6.3.13",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "0.1.11",
    "browser-sync": "2.10.1",
    "browser-sync-webpack-plugin": "1.0.1",
    "extract-text-webpack-plugin": "0.9.1",
    "faucet": "0.0.1",
    "jscs": "2.10.1",
    "jshint": "2.9.1",
    "lodash": "3.10.0",
    "node-libs-browser": "0.5.3",
    "once": "1.3.3",
    "split": "1.0.0",
    "style-loader": "0.13.0",
    "tap-difflet": "0.4.0",
    "tap-nyan": "0.0.2",
    "through2": "2.0.0",
    "webpack": "1.12.9"
  }
}
